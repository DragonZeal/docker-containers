<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>UniFi-Video</Name>
  <Description></Description>
  <Registry>https://registry.hub.docker.com/u/pducharme/unifi-video/</Registry>
  <Repository>pducharme/unifi-video</Repository>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
           22
      <Port>
        <HostPort>1935</HostPort>
        <ContainerPort>1935</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>7443</HostPort>
        <ContainerPort>7443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>7080</HostPort>
        <ContainerPort>7080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>6666</HostPort>
        <ContainerPort>6666</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>554</HostPort>
        <ContainerPort>554</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>      
      <Port>
        <HostPort>7447</HostPort>
        <ContainerPort>7447</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8880</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>      
      <Port>
        <HostPort>4443</HostPort>
        <ContainerPort>443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>      
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/unifi-video-logs/</HostDir>
      <ContainerDir>/var/log/unifi-video</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/appdata/unifi-video/</HostDir>
      <ContainerDir>/var/lib/unifi-video</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>http://[IP]:[PORT:8880]/manage</WebUI>
  <Banner>http://i.imgur.com/dD9ub3n.png</Banner>
  <Icon>http://i.imgur.com/dD9ub3n.png</Icon>
</Container>
