<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>PlexConnect</Name>
  <Description>This is a Dockerfile setup for PlexConnect - https://github.com/iBaa/PlexConnect.This is a fork from needo PlexConnect docker to pull from master tree on PlexConnect project and also add PIL for Fanart support.  Please read instructions to use it because it use Port 80, you need to edit go file to move the Unraid WebUI to alternate port.You can specify Alternate DNS server (ie: Geo-Unblocker DNS like Unotelly or Unblock-Us).</Description>
  <Registry>https://registry.hub.docker.com/u/pducharme/plexconnect/</Registry>
  <Repository>pducharme/plexconnect</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
  </Environment>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>80</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>53</HostPort>
        <ContainerPort>53</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>443</HostPort>
        <ContainerPort>443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/plexconnect/</HostDir>
      <ContainerDir>/opt/plexconnect/assets/certificates</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>511136ea3c5a64f2</Version>
  <WebUI>http://[IP]:[PORT:80]/</WebUI>
  <Banner>http://i.imgur.com/aXdzX1v.png</Banner>
  <Icon>http://i.imgur.com/aXdzX1v.png</Icon>
</Container>
